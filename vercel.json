{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": { "zeroConfig": true }
      }
    ],
    "buildCommand": "npm run build",
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/main.js"
      }
    ]
  }
  
  